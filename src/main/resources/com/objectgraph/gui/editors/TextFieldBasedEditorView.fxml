<AnchorPane id="AnchorPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns:fx="http://javafx.com/fxml">
  <children>
    <HBox id="HBox" alignment="CENTER" spacing="5.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <children>
        <TextField fx:id="textField" prefWidth="200.0" HBox.hgrow="ALWAYS" />
        <Button id="okButton" fx:id="commitButton" contentDisplay="CENTER" mnemonicParsing="false" text="Commit" HBox.hgrow="NEVER" />
        <Button fx:id="cancelButton" contentDisplay="CENTER" mnemonicParsing="false" text="Cancel" HBox.hgrow="NEVER" />
      </children>
    </HBox>
  </children>
</AnchorPane>
